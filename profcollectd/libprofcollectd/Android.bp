cc_library {
    name: "libprofcollectd",

    defaults: ["libprofcollectd_defaults"],

    shared_libs: [
        "libbase",
        "libbinder",
        "libjsoncpp",
        "liblog",
        "libsimpleperf_profcollect",
        "libutils",
        "libziparchive",
    ],

    static_libs: [
