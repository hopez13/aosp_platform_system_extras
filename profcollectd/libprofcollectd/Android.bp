    srcs: [
        "binder_service.cpp",
        "compress.cpp",
        "interface.cpp",
        "scheduler.cpp",
        "simpleperf_etm_provider.cpp",
        ":profcollectd_aidl",
    ],

    export_include_dirs: ["include"],
}
