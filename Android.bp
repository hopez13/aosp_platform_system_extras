cc_defaults {
    name: "system-extras-cflags-defaults",
    cflags: [
        "-Wall",
        "-Werror",
        "-Wno-unused-parameter",
        "-Wno-unused-result",
        "-Wno-unused-variable",
        // There are some -Wformat errors to fix, for the mac target
        "-Wno-error=format",
    ],
    clang_cflags: [
        "-Wno-incompatible-pointer-types-discards-qualifiers",
        "-Wno-unused-const-variable",
    ],
}

subdirs = ["*"]
